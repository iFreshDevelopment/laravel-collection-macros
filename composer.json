{
    "name": "ifresh/collection-macros",
    "description": "A collection of various useful additions to the Laravel collection class",
    "type": "library",
    "require": {
        "php": "^8.0"
    },
    "authors": [
        {
            "name": "Tom Kuijer",
            "email": "t.kuijer@ifresh.nl"
        },
        {
            "name": "Stephan Bouman",
            "email": "s.bouman@ifresh.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "IFresh\\Collection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IFresh\\Collection\\CollectionServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.1"
    }
}
